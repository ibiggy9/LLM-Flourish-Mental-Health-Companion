{
  "expo": {
    "name": "Flourish",
    "slug": "floursh-app",
    "scheme": "floursh-app",
    "plugins": [
      "expo-apple-authentication",
      "@react-native-firebase/app",
      "@react-native-firebase/perf",
      "@react-native-google-signin/google-signin",
      
      [
      "expo-screen-orientation",
      
        {
          "initialOrientation": "PORTRAIT_UP"
        }],
      "@react-native-firebase/crashlytics",
      [
        "expo-build-properties",
         {
          "ios": {
             "useFrameworks": "static"
           }
         }
       ]
      
    ],
    "privacy": "unlisted",
    "version": "1.9.2",
    "orientation": "portrait",
    "icon": "./assets/flourishIcon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/FlourishSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.flourish.theFlourishApplication",
      "buildNumber":"1.6.1",
      "requireFullScreen": true,
      "googleServicesFile": "./GoogleService-info.plist",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "androidStatusBar":{
      "translucent":true
  },
    "android": {
      "versionCode":71,
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode" : "resize",
      "adaptiveIcon": {
        "foregroundImage": "./assets/flourishIcon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.flourish.theFlourishApplication"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a7eb187b-35fa-4453-89fe-cec2601d10d4"
      }
    },
  
    "owner": "ibiggy"
  }
}
